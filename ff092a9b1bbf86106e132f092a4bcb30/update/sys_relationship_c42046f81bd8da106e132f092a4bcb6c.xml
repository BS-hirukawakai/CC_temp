        <name>同一のメールアドレス</name>
        <query_from/>
        <query_with><![CDATA[(function refineQuery(current, parent) {
    var sysIds = '';
    var gr_same = new GlideRecord("x_bllt_bs_cc_inquiry");
    gr_same.addQuery("u_mailaddress", parent.u_mailaddress);
    gr_same.addNotNullQuery('u_mailaddress');
    gr_same.query();

    while (gr_same.next()) {
        if (gr_same.sys_id == parent.sys_id) {
            continue;
        } else {
            sysIds += gr_same.sys_id + ',';
        }
    }

    if (sysIds != '') {
        sysIds = sysIds.substring(0, sysIds.length - 1);
    }
    current.addQuery('sys_id', 'IN', sysIds);

})(current, parent);]]></query_with>
